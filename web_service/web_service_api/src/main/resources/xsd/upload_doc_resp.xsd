<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://asander.ru/ws/UploadDoc/resp"
		   xmlns:xs="http://www.w3.org/2001/XMLSchema"
		   xmlns="http://asander.ru/ws/UploadDoc/resp"
		   xmlns:result="http://asander.ru/ws/common/result"
		   version="20190610">
	<xs:annotation>
		<xs:documentation>Данное интеграционное сообщение описывает ответ на запрос создания нового документа клиента в ИС</xs:documentation>
	</xs:annotation>
	<xs:import namespace="http://asander.ru/ws/common/result" schemaLocation="common/common_result.xsd"/>
	<xs:element name="UploadDocResp" type="UploadDocRespType">
		<xs:annotation>
			<xs:documentation>Ответ на запрос создания нового документа клиента в ИС</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="UploadDocRespType">
		<xs:annotation>
			<xs:documentation>Ответ на запрос создания нового документа клиента в ИС</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ClientID" type="xs:ID"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>ID созданного/найденного клиента(каталога) в АСОА, под которым будет сохранен документ.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DossierID" type="xs:ID"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>ID созданного/найденного досъе(каталога) в АСОА, под которым будет сохранен документ.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ResultData" type="result:IntegrationSimpleResultDataType">
				<xs:annotation>
					<xs:documentation>Блок результата обработки запроса</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Document" type="DocumentType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Информация по загруженному документу в АСОА</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DocumentType">
		<xs:annotation>
			<xs:documentation>Тип описания информации по загруженному документу в АСОА</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DocID" type="xs:ID" minOccurs="0">
				<xs:annotation>
					<xs:documentation>ID документа в АСОА</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DocURL" type="xs:anyURI" minOccurs="0">
				<xs:annotation>
					<xs:documentation>URL на просмотр электронного образа в интерфейсе АСОА</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
